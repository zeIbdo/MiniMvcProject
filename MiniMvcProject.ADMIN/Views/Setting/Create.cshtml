@model SettingCreateViewModel

<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Create Setting</h4>
            <form class="forms-sample" method="post" enctype="multipart/form-data">
                <div asp-validation-summary="All">
                </div>
                <div class="form-group">
                    <label>Key</label>
                    <input asp-for="Key" class="form-control" placeholder="Key">
                </div>
                <div class="form-group">
                    <label>Value</label>
                    <input asp-for="Value" class="form-control" placeholder="Value">
                </div>

                <button type="submit" class="btn btn-primary mr-2">Create</button>
                <a asp-controller="setting" asp-action="index" class="btn btn-light">Cancel</a>
            </form>
        </div>
    </div>
</div>